{
    "name": "latex-template-ja",
    "dockerComposeFile": "compose.yaml",
    "service": "texlive",
    "workspaceFolder": "/workspaces/paper",
    "shutdownAction": "stopCompose",
    "customizations": {
        "vscode": {
            "settings": {
                "extensions.verifySignature": false
            },
            "extensions": [
                "james-yu.latex-workshop",
                "MS-CEINTL.vscode-language-pack-ja"
            ]
        }
    }
}
